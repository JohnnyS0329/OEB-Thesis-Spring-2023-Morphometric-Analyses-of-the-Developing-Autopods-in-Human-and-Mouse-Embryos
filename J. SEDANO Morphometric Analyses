model_extrinsic_druelle_10_offset = lm(HP5_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
  # Hind Paw Metapodials
model_extrinsic_druelle_11_offset = lm(FM1_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_12_offset = lm(FM2_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)

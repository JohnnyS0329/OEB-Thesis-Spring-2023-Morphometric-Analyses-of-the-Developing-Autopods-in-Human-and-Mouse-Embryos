## Import Human Autopod Data from Google Sheet
```{r, include=FALSE}
## Retrieve Recent Data
human_autopod_data <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=645959465")
human_autopod_samples <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=1923691536")
human_autopod_data$measurement_date <- as.Date(human_autopod_data$measurement_date, format ="%m/%d/%Y")

human_measurements_key <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=3238789")

human_measurements <- colnames(human_autopod_data)[8:47]
human_autopod_data_tidy <- human_autopod_data %>% pivot_longer(all_of(human_measurements), names_to = "measurement_id") %>% select(-notes)%>% select(-timepoint)
human_autopod_data_tidy_merged <- merge(human_autopod_data_tidy, human_measurements_key)
human_autopod_data_tidy_merged <- merge(human_autopod_data_tidy_merged, human_autopod_samples, by= 'sample_id')
human_autopod_data_tidy_merged_avg <- human_autopod_data_tidy_merged %>% filter(value != 'NA') %>%  group_by(element, sex, timepoint, sample_id, measurement_date, measurer) %>% summarise_at(vars(value), list(value = mean))
```

## Import Mouse Autopod Data from Google Sheet
```{r, include=FALSE}
## Retrieve Recent Data
mouse_autopod_data <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=1826500199")
mouse_autopod_samples <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=597198010")
mouse_autopod_data$measurement_date <- as.Date(mouse_autopod_data$measurement_date, format ="%m/%d/%Y")

mouse_measurements_key <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=3238789")

mouse_measurements <- colnames(mouse_autopod_data)[8:47]
mouse_autopod_data_tidy <- mouse_autopod_data %>% pivot_longer(all_of(mouse_measurements), names_to = "measurement_id") %>% select(-notes)%>% select(-timepoint)
mouse_autopod_data_tidy_merged <- merge(mouse_autopod_data_tidy, mouse_measurements_key)
mouse_autopod_data_tidy_merged <- merge(mouse_autopod_data_tidy_merged, mouse_autopod_samples, by= 'sample_id')
mouse_autopod_data_tidy_merged_avg <- mouse_autopod_data_tidy_merged %>% filter(value != 'NA') %>%  group_by(element, sex, timepoint, sample_id, measurement_date, measurer) %>% summarise_at(vars(value), list(value = mean))
```

## Human Embryonic Autopod Measurements
```{r, include=FALSE}
# Create a data frame of averaged length data for each side
human_autopod_data_avg <- human_autopod_data[1:7]
human_autopod_data_avg$HP1_avg <- rowMeans(human_autopod_data[,c('HP1_R_len','HP1_L_len')], na.rm = TRUE)
human_autopod_data_avg$HP2_avg <- rowMeans(human_autopod_data[,c('HP2_R_len','HP2_L_len')], na.rm = TRUE)
human_autopod_data_avg$HP3_avg <- rowMeans(human_autopod_data[,c('HP3_R_len','HP3_L_len')], na.rm = TRUE)
human_autopod_data_avg$HP4_avg <- rowMeans(human_autopod_data[,c('HP4_R_len','HP4_L_len')], na.rm = TRUE)
human_autopod_data_avg$HP5_avg <- rowMeans(human_autopod_data[,c('HP5_R_len','HP5_L_len')], na.rm = TRUE)
human_autopod_data_avg$HM1_avg <- rowMeans(human_autopod_data[,c('HM1_R_len','HM1_L_len')], na.rm = TRUE)
human_autopod_data_avg$HM2_avg <- rowMeans(human_autopod_data[,c('HM2_R_len','HM2_L_len')], na.rm = TRUE)
human_autopod_data_avg$HM3_avg <- rowMeans(human_autopod_data[,c('HM3_R_len','HM3_L_len')], na.rm = TRUE)
human_autopod_data_avg$HM4_avg <- rowMeans(human_autopod_data[,c('HM4_R_len','HM4_L_len')], na.rm = TRUE)
human_autopod_data_avg$HM5_avg <- rowMeans(human_autopod_data[,c('HM5_R_len','HM5_L_len')], na.rm = TRUE)
human_autopod_data_avg$FP1_avg <- rowMeans(human_autopod_data[,c('FP1_R_len','FP1_L_len')], na.rm = TRUE)
human_autopod_data_avg$FP2_avg <- rowMeans(human_autopod_data[,c('FP2_R_len','FP2_L_len')], na.rm = TRUE)
human_autopod_data_avg$FP3_avg <- rowMeans(human_autopod_data[,c('FP3_R_len','FP3_L_len')], na.rm = TRUE)
human_autopod_data_avg$FP4_avg <- rowMeans(human_autopod_data[,c('FP4_R_len','FP4_L_len')], na.rm = TRUE)
human_autopod_data_avg$FP5_avg <- rowMeans(human_autopod_data[,c('FP5_R_len','FP5_L_len')], na.rm = TRUE)
human_autopod_data_avg$FM1_avg <- rowMeans(human_autopod_data[,c('FM1_R_len','FM1_L_len')], na.rm = TRUE)
human_autopod_data_avg$FM2_avg <- rowMeans(human_autopod_data[,c('FM2_R_len','FM2_L_len')], na.rm = TRUE)
human_autopod_data_avg$FM3_avg <- rowMeans(human_autopod_data[,c('FM3_R_len','FM3_L_len')], na.rm = TRUE)
human_autopod_data_avg$FM4_avg <- rowMeans(human_autopod_data[,c('FM4_R_len','FM4_L_len')], na.rm = TRUE)
human_autopod_data_avg$FM5_avg <- rowMeans(human_autopod_data[,c('FM5_R_len','FM5_L_len')], na.rm = TRUE)
```


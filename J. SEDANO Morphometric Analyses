m = summary(model_extrinsic_druelle_14)
  mean_extrinsic_druelle_14 <- m$coefficients[2,1]
  upper_bound_extrinsic_druelle_14 <- confint(model_extrinsic_druelle_14, "Crown_rump_length")[2]
  lower_bound_extrinsic_druelle_14 <- confint(model_extrinsic_druelle_14, "Crown_rump_length")[1]
n = summary(model_extrinsic_druelle_14_offset)  
  t_value_extrinsic_druelle_14 <- n$coefficients[2,3]
  p_value_extrinsic_druelle_14_human <- n$coefficients[2,4]
  

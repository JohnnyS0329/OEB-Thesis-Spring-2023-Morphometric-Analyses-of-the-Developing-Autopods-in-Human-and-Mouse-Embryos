# Manual Digits Ontogenetic Allometry Plot
Druelle_Manual_Comparison_to_CRL <- data.frame(Mean = c(mean_extrinsic_druelle_1, mean_extrinsic_druelle_6, mean_extrinsic_druelle_2, mean_extrinsic_druelle_7, mean_extrinsic_druelle_3, mean_extrinsic_druelle_8, mean_extrinsic_druelle_4, mean_extrinsic_druelle_9, mean_extrinsic_druelle_5, mean_extrinsic_druelle_10),
  Lower_Bound = c(lower_bound_extrinsic_druelle_1, lower_bound_extrinsic_druelle_6, lower_bound_extrinsic_druelle_2, lower_bound_extrinsic_druelle_7, lower_bound_extrinsic_druelle_3, lower_bound_extrinsic_druelle_8, lower_bound_extrinsic_druelle_4, lower_bound_extrinsic_druelle_9, lower_bound_extrinsic_druelle_5, lower_bound_extrinsic_druelle_10),
  Upper_Bound = c(upper_bound_extrinsic_druelle_1, upper_bound_extrinsic_druelle_6, upper_bound_extrinsic_druelle_2, upper_bound_extrinsic_druelle_7, upper_bound_extrinsic_druelle_3, upper_bound_extrinsic_druelle_8, upper_bound_extrinsic_druelle_4, upper_bound_extrinsic_druelle_9, upper_bound_extrinsic_druelle_5, upper_bound_extrinsic_druelle_10),
  Autopod_Type = rep(c("Metapodial", "Phalange"), 5),
  grp = c(rep("DIGIT 1", 2),rep("DIGIT 2", 2),rep("DIGIT 3", 2),rep("DIGIT 4", 2),rep("DIGIT 5", 2))
  )

Druelle_Manual_Comparison_to_CRL %>%
ggplot(aes(x = grp, y = Mean, groups = Autopod_Type, fill = Autopod_Type)) +
  stat_summary(fun = mean, geom = "bar", position = position_dodge()) +
geom_errorbar(aes(ymin = Lower_Bound, ymax = Upper_Bound), width = 0.2, position = position_dodge(0.9)) +
  geom_hline(yintercept = 1) +
  labs (y = "Scaling Exponent", x = "", fill = "Autopod Type") +
  ggtitle ("Human Manual Digits") +
  theme(plot.title = element_text(face = "bold")) +
  theme(axis.text.x = element_text(face = "bold")) +
  theme(plot.title = element_text(hjust = 0.5)) +
  scale_y_continuous(expand=c(0,0))

# Pedal Digits Ontogenetic Allometry Plot
Druelle_Pedal_Comparison_to_CRL <- data.frame(Mean = c(mean_extrinsic_druelle_11, mean_extrinsic_druelle_16, mean_extrinsic_druelle_12, mean_extrinsic_druelle_17, mean_extrinsic_druelle_13, mean_extrinsic_druelle_18, mean_extrinsic_druelle_14, mean_extrinsic_druelle_19, mean_extrinsic_druelle_15, mean_extrinsic_druelle_20),

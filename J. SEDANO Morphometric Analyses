## Ontogenetic Allometry Graphs - Mouse
```{r}
# Formatting the Data for Tidyverse
# Renaming
mouse_autopod_data_extrinsic_graph <- mouse_autopod_data_z_score %>% 
        rename("HP1" = "HP1_z_score",
               "HP2" = "HP2_z_score",
               "HP3" = "HP3_z_score",
               "HP4" = "HP4_z_score",
               "HP5" = "HP5_z_score",
               "HM1" = "HM1_z_score",
               "HM2" = "HM2_z_score",
               "HM3" = "HM3_z_score",
               "HM4" = "HM4_z_score",
               "HM5" = "HM5_z_score",
               "FP1" = "FP1_z_score",
               "FP2" = "FP2_z_score",
               "FP3" = "FP3_z_score",
               "FP4" = "FP4_z_score",
               "FP5" = "FP5_z_score",
               "FM1" = "FM1_z_score",
               "FM2" = "FM2_z_score",
               "FM3" = "FM3_z_score",
               "FM4" = "FM4_z_score",
               "FM5" = "FM5_z_score")
  # Tidyverse
mouse_measurements <- colnames(mouse_autopod_data_extrinsic_graph)[8:27]
mouse_autopod_data_tidy_z_score <- mouse_autopod_data_extrinsic_graph %>% pivot_longer(all_of(mouse_measurements), names_to = "measurement_id") %>% select(-notes) %>% select(-timepoint)

# Facet-Wrap() for Ontogenetic Allometry
ggplot(data = mouse_autopod_data_tidy_z_score, aes(Crown_rump_length, value)) +
  geom_point(color = "gold") + 
  labs(title = "Mouse Allometry",
       y = "Z-score Transformed Length", x = "Z-score Transformed Crown-rump Length") + 
  facet_wrap(~ measurement_id, nrow = 4)  + stat_smooth(method = 'lm', formula = y ~ x, geom = "smooth", color = "gold")
```

## Summation Plots & Tables of Extrinsic Measurements - Mouse
```{r}
# Manual Digits Ontogenetic Allometry Plot
Druelle_Manual_Comparison_to_CRL <- data.frame(Mean = c(mean_extrinsic_druelle_1, mean_extrinsic_druelle_6, mean_extrinsic_druelle_2, mean_extrinsic_druelle_7, mean_extrinsic_druelle_3, mean_extrinsic_druelle_8, mean_extrinsic_druelle_4, mean_extrinsic_druelle_9, mean_extrinsic_druelle_5, mean_extrinsic_druelle_10),
  Lower_Bound = c(lower_bound_extrinsic_druelle_1, lower_bound_extrinsic_druelle_6, lower_bound_extrinsic_druelle_2, lower_bound_extrinsic_druelle_7, lower_bound_extrinsic_druelle_3, lower_bound_extrinsic_druelle_8, lower_bound_extrinsic_druelle_4, lower_bound_extrinsic_druelle_9, lower_bound_extrinsic_druelle_5, lower_bound_extrinsic_druelle_10),
  Upper_Bound = c(upper_bound_extrinsic_druelle_1, upper_bound_extrinsic_druelle_6, upper_bound_extrinsic_druelle_2, upper_bound_extrinsic_druelle_7, upper_bound_extrinsic_druelle_3, upper_bound_extrinsic_druelle_8, upper_bound_extrinsic_druelle_4, upper_bound_extrinsic_druelle_9, upper_bound_extrinsic_druelle_5, upper_bound_extrinsic_druelle_10),
  Autopod_Type = rep(c("Metapodial", "Phalange"), 5),
  grp = c(rep("DIGIT 1", 2),rep("DIGIT 2", 2),rep("DIGIT 3", 2),rep("DIGIT 4", 2),rep("DIGIT 5", 2))
  )

Druelle_Manual_Comparison_to_CRL %>%
ggplot(aes(x = grp, y = Mean, groups = Autopod_Type, fill = Autopod_Type)) +
  stat_summary(fun = mean, geom = "bar", position = position_dodge()) +
geom_errorbar(aes(ymin = Lower_Bound, ymax = Upper_Bound), width = 0.2, position = position_dodge(0.9)) +
  geom_hline(yintercept = 1) +
  labs (y = "Scaling Exponent", x = "", fill = "Autopod Type") +
  ggtitle ("Mouse Manual Digits") +
  theme(plot.title = element_text(face = "bold")) +
  theme(axis.text.x = element_text(face = "bold")) +
  theme(plot.title = element_text(hjust = 0.5)) +
  scale_y_continuous(expand=c(0,0))

# Pedal Digits Ontogenetic Allometry Plot
Druelle_Pedal_Comparison_to_CRL <- data.frame(Mean = c(mean_extrinsic_druelle_11, mean_extrinsic_druelle_16, mean_extrinsic_druelle_12, mean_extrinsic_druelle_17, mean_extrinsic_druelle_13, mean_extrinsic_druelle_18, mean_extrinsic_druelle_14, mean_extrinsic_druelle_19, mean_extrinsic_druelle_15, mean_extrinsic_druelle_20),
  Lower_Bound = c(lower_bound_extrinsic_druelle_11, lower_bound_extrinsic_druelle_16, lower_bound_extrinsic_druelle_12, lower_bound_extrinsic_druelle_17, lower_bound_extrinsic_druelle_13, lower_bound_extrinsic_druelle_18, lower_bound_extrinsic_druelle_14, lower_bound_extrinsic_druelle_19, lower_bound_extrinsic_druelle_15, lower_bound_extrinsic_druelle_20),
  Upper_Bound = c(upper_bound_extrinsic_druelle_11, upper_bound_extrinsic_druelle_16, upper_bound_extrinsic_druelle_12, upper_bound_extrinsic_druelle_17, upper_bound_extrinsic_druelle_13, upper_bound_extrinsic_druelle_18, upper_bound_extrinsic_druelle_14, upper_bound_extrinsic_druelle_19, upper_bound_extrinsic_druelle_15, upper_bound_extrinsic_druelle_20),
  Autopod_Type = rep(c("Metapodial", "Phalange"), 5),
  grp = c(rep("DIGIT 1", 2),rep("DIGIT 2", 2),rep("DIGIT 3", 2),rep("DIGIT 4", 2),rep("DIGIT 5", 2))
  )

Druelle_Pedal_Comparison_to_CRL %>%
ggplot(aes(x = grp, y = Mean, groups = Autopod_Type, fill = Autopod_Type)) +
  stat_summary(fun = mean, geom = "bar", position = position_dodge()) +
geom_errorbar(aes(ymin = Lower_Bound, ymax = Upper_Bound), width = 0.2, position = position_dodge(0.9)) +
  geom_hline(yintercept = 1) +
  labs (y = "Scaling Exponent", x = "", fill = "Autopod Type") +
  ggtitle ("Mouse Pedal Digits") +
  theme(plot.title = element_text(face = "bold")) +
  theme(axis.text.x = element_text(face = "bold")) + 
  theme(plot.title = element_text(hjust = 0.5)) +
  scale_y_continuous(expand=c(0,0))

# Ontogenetic Allometry Table
  # Manual Digits Table 
    # Adding Autopod Element Names
Druelle_Manual_Comparison_to_CRL$Autopod_Element <- c("Metapodial 1", "Phalange 1", "Metapodial 2", "Phalange 2", "Metapodial 3", "Phalange 3", "Metapodial 4", "Phalange 4", "Metapodial 5", "Phalange 5")
Druelle_Manual_Comparison_to_CRL <- Druelle_Manual_Comparison_to_CRL[, c(6,1,2,3,4,5)]
    # Subtracting Unnecessary Columns
Druelle_Manual_Comparison_to_CRL <- Druelle_Manual_Comparison_to_CRL[,-c(5,6)]
    # Extracting T-Value
Druelle_Manual_Comparison_to_CRL$T_Value <- c(t_value_extrinsic_druelle_1, t_value_extrinsic_druelle_6, t_value_extrinsic_druelle_2,
t_value_extrinsic_druelle_7, t_value_extrinsic_druelle_3,
t_value_extrinsic_druelle_8, t_value_extrinsic_druelle_4,
t_value_extrinsic_druelle_9, t_value_extrinsic_druelle_5,
t_value_extrinsic_druelle_10)
    # Extracting P-Value
Druelle_Manual_Comparison_to_CRL$P_Value <- c(p_value_extrinsic_druelle_1_mouse, p_value_extrinsic_druelle_6_mouse, p_value_extrinsic_druelle_2_mouse,
p_value_extrinsic_druelle_7_mouse, p_value_extrinsic_druelle_3_mouse,
p_value_extrinsic_druelle_8_mouse, p_value_extrinsic_druelle_4_mouse,
p_value_extrinsic_druelle_9_mouse, p_value_extrinsic_druelle_5_mouse,
p_value_extrinsic_druelle_10_mouse)

  # Pedal Digits Table 
    # Adding Autopod Element Names
Druelle_Pedal_Comparison_to_CRL$Autopod_Element <- c("Metapodial 1", "Phalange 1", "Metapodial 2", "Phalange 2", "Metapodial 3", "Phalange 3", "Metapodial 4", "Phalange 4", "Metapodial 5", "Phalange 5")
Druelle_Pedal_Comparison_to_CRL <- Druelle_Pedal_Comparison_to_CRL[, c(6,1,2,3,4,5)]
    # Subtracting Unnecessary Columns
Druelle_Pedal_Comparison_to_CRL <- Druelle_Pedal_Comparison_to_CRL[,-c(5,6)]
    # Extracting R-Squared Value
Druelle_Pedal_Comparison_to_CRL$T_Value <- c(t_value_extrinsic_druelle_11, t_value_extrinsic_druelle_16, t_value_extrinsic_druelle_12,
t_value_extrinsic_druelle_17, t_value_extrinsic_druelle_13,
t_value_extrinsic_druelle_18, t_value_extrinsic_druelle_14,
t_value_extrinsic_druelle_19, t_value_extrinsic_druelle_15,
t_value_extrinsic_druelle_20)
    # Extracting P-Value
Druelle_Pedal_Comparison_to_CRL$P_Value <- c(p_value_extrinsic_druelle_11_mouse, p_value_extrinsic_druelle_16_mouse, p_value_extrinsic_druelle_12_mouse, 
p_value_extrinsic_druelle_17_mouse, p_value_extrinsic_druelle_13_mouse,
p_value_extrinsic_druelle_18_mouse, p_value_extrinsic_druelle_14_mouse,
p_value_extrinsic_druelle_19_mouse, p_value_extrinsic_druelle_15_mouse,
p_value_extrinsic_druelle_20_mouse)

# Saving Tables as Mouse-Specific
Mouse_Manual_Comparison_to_CRL <- Druelle_Manual_Comparison_to_CRL
Mouse_Pedal_Comparison_to_CRL <- Druelle_Pedal_Comparison_to_CRL
```


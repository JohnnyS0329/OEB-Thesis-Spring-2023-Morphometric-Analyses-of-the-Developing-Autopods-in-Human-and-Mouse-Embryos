## Intrinsic Measure - Mouse
```{r}
# Intrinsic Measure: Ontogenetic Scaling of Manual and Pedal Phalangeal Indices [RECREATION OF FIGURES 5 & 6]
  # In terms of the ontogenetic scaling for this paper, Young focuses on comparing the Phalangeal Indices in comparison to Relative Age. For this R script I just decided to stick with comparing the Phalangeal Indices in comparison to the Actual Age also known as the Embryonic Day in this comparison.

  # The formula for the this version of the "Phalangeal Index" is:
    # Phalangeal Index = (Length of a Ray's Phalanges)/(Length of a Ray's Metapodial)

  # Formatting the Data
mouse_autopod_data_intrinsic <- mouse_autopod_data_avg[1:7]

mouse_autopod_data_intrinsic$MOUSE_MANUAL_DIGIT_1 <- ((mouse_autopod_data_avg$HP1_avg/mouse_autopod_data_avg$HM1_avg))
mouse_autopod_data_intrinsic$MOUSE_MANUAL_DIGIT_2 <- ((mouse_autopod_data_avg$HP2_avg/mouse_autopod_data_avg$HM2_avg))
mouse_autopod_data_intrinsic$MOUSE_MANUAL_DIGIT_3 <- ((mouse_autopod_data_avg$HP3_avg/mouse_autopod_data_avg$HM3_avg))
mouse_autopod_data_intrinsic$MOUSE_MANUAL_DIGIT_4 <- ((mouse_autopod_data_avg$HP4_avg/mouse_autopod_data_avg$HM4_avg))
mouse_autopod_data_intrinsic$MOUSE_MANUAL_DIGIT_5 <- ((mouse_autopod_data_avg$HP5_avg/mouse_autopod_data_avg$HM5_avg))

mouse_autopod_data_intrinsic$MOUSE_PEDAL_DIGIT_1 <- ((mouse_autopod_data_avg$FP1_avg/mouse_autopod_data_avg$FM1_avg))
mouse_autopod_data_intrinsic$MOUSE_PEDAL_DIGIT_2 <- ((mouse_autopod_data_avg$FP2_avg/mouse_autopod_data_avg$FM2_avg))
mouse_autopod_data_intrinsic$MOUSE_PEDAL_DIGIT_3 <- ((mouse_autopod_data_avg$FP3_avg/mouse_autopod_data_avg$FM3_avg))
mouse_autopod_data_intrinsic$MOUSE_PEDAL_DIGIT_4 <- ((mouse_autopod_data_avg$FP4_avg/mouse_autopod_data_avg$FM4_avg))
mouse_autopod_data_intrinsic$MOUSE_PEDAL_DIGIT_5 <- ((mouse_autopod_data_avg$FP5_avg/mouse_autopod_data_avg$FM5_avg))

mouse_autopod_data_intrinsic$timepoint <- as.numeric(as.character(mouse_autopod_data_intrinsic$timepoint))

# Making the Models
    # Manual Digit 1 PI
model_intrinsic_young_1 = lm(MOUSE_MANUAL_DIGIT_1 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Manual Digit 2 PI
model_intrinsic_young_2 = lm(MOUSE_MANUAL_DIGIT_2 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Manual Digit 3 PI
model_intrinsic_young_3 = lm(MOUSE_MANUAL_DIGIT_3 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Manual Digit 4 PI
model_intrinsic_young_4 = lm(MOUSE_MANUAL_DIGIT_4 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Manual Digit 5 PI
model_intrinsic_young_5 = lm(MOUSE_MANUAL_DIGIT_5 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Pedal Digit 1 PI
model_intrinsic_young_6 = lm(MOUSE_PEDAL_DIGIT_1 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Pedal Digit 2 PI
model_intrinsic_young_7 = lm(MOUSE_PEDAL_DIGIT_2 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Pedal Digit 3 PI
model_intrinsic_young_8 = lm(MOUSE_PEDAL_DIGIT_3 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Pedal Digit 4 PI
model_intrinsic_young_9 = lm(MOUSE_PEDAL_DIGIT_4 ~ timepoint, data = mouse_autopod_data_intrinsic)
    # Pedal Digit 5 PI
model_intrinsic_young_10 = lm(MOUSE_PEDAL_DIGIT_5 ~ timepoint, data = mouse_autopod_data_intrinsic)

# Extracting Numbers
m = summary(model_intrinsic_young_1)
  mean_intrinsic_young_1 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_1 <- confint(model_intrinsic_young_1, "timepoint")[2]
  lower_bound_intrinsic_young_1 <- confint(model_intrinsic_young_1, "timepoint")[1]
  t_value_intrinsic_young_1 <- m$coefficients[2,3]
  p_value_intrinsic_young_1_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_2)
  mean_intrinsic_young_2 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_2 <- confint(model_intrinsic_young_2, "timepoint")[2]
  lower_bound_intrinsic_young_2 <- confint(model_intrinsic_young_2, "timepoint")[1]
  t_value_intrinsic_young_2 <- m$coefficients[2,3]
  p_value_intrinsic_young_2_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_3)
  mean_intrinsic_young_3 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_3 <- confint(model_intrinsic_young_3, "timepoint")[2]
  lower_bound_intrinsic_young_3 <- confint(model_intrinsic_young_3, "timepoint")[1]
  t_value_intrinsic_young_3 <- m$coefficients[2,3]
  p_value_intrinsic_young_3_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_4)
  mean_intrinsic_young_4 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_4 <- confint(model_intrinsic_young_4, "timepoint")[2]
  lower_bound_intrinsic_young_4 <- confint(model_intrinsic_young_4, "timepoint")[1]
  t_value_intrinsic_young_4 <- m$coefficients[2,3]
  p_value_intrinsic_young_4_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_5)
  mean_intrinsic_young_5 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_5 <- confint(model_intrinsic_young_5, "timepoint")[2]
  lower_bound_intrinsic_young_5 <- confint(model_intrinsic_young_5, "timepoint")[1]
  t_value_intrinsic_young_5 <- m$coefficients[2,3]
  p_value_intrinsic_young_5_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_6)
  mean_intrinsic_young_6 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_6 <- confint(model_intrinsic_young_6, "timepoint")[2]
  lower_bound_intrinsic_young_6 <- confint(model_intrinsic_young_6, "timepoint")[1]
  t_value_intrinsic_young_6 <- m$coefficients[2,3]
  p_value_intrinsic_young_6_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_7)
  mean_intrinsic_young_7 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_7 <- confint(model_intrinsic_young_7, "timepoint")[2]
  lower_bound_intrinsic_young_7 <- confint(model_intrinsic_young_7, "timepoint")[1]
  t_value_intrinsic_young_7 <- m$coefficients[2,3]
  p_value_intrinsic_young_7_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_8)
  mean_intrinsic_young_8 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_8 <- confint(model_intrinsic_young_8, "timepoint")[2]
  lower_bound_intrinsic_young_8 <- confint(model_intrinsic_young_8, "timepoint")[1]
  t_value_intrinsic_young_8 <- m$coefficients[2,3]
  p_value_intrinsic_young_8_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_9)
  mean_intrinsic_young_9 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_9 <- confint(model_intrinsic_young_9, "timepoint")[2]
  lower_bound_intrinsic_young_9 <- confint(model_intrinsic_young_9, "timepoint")[1]
  t_value_intrinsic_young_9 <- m$coefficients[2,3]
  p_value_intrinsic_young_9_mouse <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_10)
  mean_intrinsic_young_10 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_10 <- confint(model_intrinsic_young_10, "timepoint")[2]
  lower_bound_intrinsic_young_10 <- confint(model_intrinsic_young_10, "timepoint")[1]
  t_value_intrinsic_young_10 <- m$coefficients[2,3]
  p_value_intrinsic_young_10_mouse <- m$coefficients[2,4]
```

## Ontogenetic Changes in Phalangeal Indices Graphs - Mouse
```{r}
# Formatting the Data for Tidyverse
  # Renaming
mouse_autopod_data_intrinsic_graph <- mouse_autopod_data_intrinsic %>% 
        rename("MANUAL DIGIT 1" = "MOUSE_MANUAL_DIGIT_1",
               "MANUAL DIGIT 2" = "MOUSE_MANUAL_DIGIT_2",
               "MANUAL DIGIT 3" = "MOUSE_MANUAL_DIGIT_3",
               "MANUAL DIGIT 4" = "MOUSE_MANUAL_DIGIT_4",
               "MANUAL DIGIT 5" = "MOUSE_MANUAL_DIGIT_5",
               "PEDAL DIGIT 1" = "MOUSE_PEDAL_DIGIT_1",
               "PEDAL DIGIT 2" = "MOUSE_PEDAL_DIGIT_2",
               "PEDAL DIGIT 3" = "MOUSE_PEDAL_DIGIT_3",
               "PEDAL DIGIT 4" = "MOUSE_PEDAL_DIGIT_4",
               "PEDAL DIGIT 5" = "MOUSE_PEDAL_DIGIT_5")
  # Tidyverse
mouse_measurements <- colnames(mouse_autopod_data_intrinsic_graph)[8:17]
mouse_autopod_data_tidy_intrinsic <- mouse_autopod_data_intrinsic_graph %>% pivot_longer(all_of(mouse_measurements), names_to = "measurement_id")

# Facet-Wrap() for Ontogenetic Changes in Phalangeal Indices
ggplot(data = mouse_autopod_data_tidy_intrinsic, aes(timepoint, value)) +
  geom_point(color="gold") + 
  labs(y = "Phalangeal Indices", x = "Embryonic Timepoint (Day)") + 
  facet_wrap(~ measurement_id, nrow = 2)  + stat_smooth(method = 'lm', formula = y ~ x, geom = "smooth", color="gold")
```

## Summation Plots & Tables of Intrinsic Measurements - Mouse
```{r}
Young_Manual_Phalangeal_Indices <- data.frame(Autopod_Digit_Number = c("MANUAL DIGIT 1","MANUAL DIGIT 2", "MANUAL DIGIT 3", "MANUAL DIGIT 4", "MANUAL DIGIT 5"),
  Mean = c(mean_intrinsic_young_1, mean_intrinsic_young_2, mean_intrinsic_young_3, mean_intrinsic_young_4, mean_intrinsic_young_5),
  Upper_Bound = c(upper_bound_intrinsic_young_1, upper_bound_intrinsic_young_2, upper_bound_intrinsic_young_3, upper_bound_intrinsic_young_4, upper_bound_intrinsic_young_5),
  Lower_Bound = c(lower_bound_intrinsic_young_1, lower_bound_intrinsic_young_2, lower_bound_intrinsic_young_3, lower_bound_intrinsic_young_4, lower_bound_intrinsic_young_5)
  )

ggplot(Young_Manual_Phalangeal_Indices) +
  geom_bar(aes(x = Autopod_Digit_Number, y = Mean), stat = 'identity', fill = 'gold1') +
  geom_errorbar(aes(x = Autopod_Digit_Number, ymin = Lower_Bound, ymax = Upper_Bound), width = 0.4) +
  geom_hline(yintercept = 0) +
  labs (y = "Slope (PI versus Embryonic Timepoint)", x = "") +
  scale_x_discrete(labels=c("DIGIT 1", "DIGIT 2", "DIGIT 3", "DIGIT 4", "DIGIT 5")) +
  ggtitle ("Mouse Manual Digits") + 
  theme(plot.title = element_text(face = "bold")) +
  theme(axis.text.x = element_text(face = "bold")) +
  theme(plot.title = element_text(hjust = 0.5))

Young_Pedal_Phalangeal_Indices <- data.frame(Autopod_Digit_Number = c("PEDAL DIGIT 1", "PEDAL DIGIT 2", "PEDAL DIGIT 3", "PEDAL DIGIT 4", "PEDAL DIGIT 5"),
  Mean = c(mean_intrinsic_young_6, mean_intrinsic_young_7, mean_intrinsic_young_8, mean_intrinsic_young_9, mean_intrinsic_young_10),
  Upper_Bound = c(upper_bound_intrinsic_young_6, upper_bound_intrinsic_young_7, upper_bound_intrinsic_young_8, upper_bound_intrinsic_young_9, upper_bound_intrinsic_young_10),
  Lower_Bound = c(lower_bound_intrinsic_young_6, lower_bound_intrinsic_young_7, lower_bound_intrinsic_young_8, lower_bound_intrinsic_young_9, lower_bound_intrinsic_young_10)
  )

ggplot(Young_Pedal_Phalangeal_Indices) +
  geom_bar(aes(x = Autopod_Digit_Number, y = Mean), stat = 'identity', fill = 'gold3') +
  geom_errorbar(aes(x = Autopod_Digit_Number, ymin = Lower_Bound, ymax = Upper_Bound), width = 0.4) +
  geom_hline(yintercept = 0) +
  labs (y = "Slope (PI versus Embryonic Timepoint)", x = "") +
    scale_x_discrete(labels=c("DIGIT 1", "DIGIT 2", "DIGIT 3", "DIGIT 4", "DIGIT 5")) +
  ggtitle ("Mouse Pedal Digits") + 
  theme(plot.title = element_text(face = "bold")) +
  theme(axis.text.x = element_text(face = "bold")) +
  theme(plot.title = element_text(hjust = 0.5))

# Manual PI Table
  # Adding Digit Names
Young_Manual_Phalangeal_Indices$Digit_Number <- c("Digit 1", "Digit 2", "Digit 3", "Digit 4", "Digit 5")
Young_Manual_Phalangeal_Indices <- Young_Manual_Phalangeal_Indices[, c(5,1,2,3,4)]
  # Removing Unnecessary Columns
Young_Manual_Phalangeal_Indices <- Young_Manual_Phalangeal_Indices[,-c(2)]
  # Renaming a Column
colnames(Young_Manual_Phalangeal_Indices)[2] ="Slope"
  # Extracting T-Value
Young_Manual_Phalangeal_Indices$T_Value <- c(t_value_intrinsic_young_1, t_value_intrinsic_young_2, t_value_intrinsic_young_3, t_value_intrinsic_young_4, t_value_intrinsic_young_5)
  # Extracting P-Values
Young_Manual_Phalangeal_Indices$P_Value <- c(p_value_intrinsic_young_1_mouse, p_value_intrinsic_young_2_mouse, p_value_intrinsic_young_3_mouse, p_value_intrinsic_young_4_mouse, p_value_intrinsic_young_5_mouse)

# Pedal PI Table
  # Adding Digit Names
Young_Pedal_Phalangeal_Indices$Digit_Number <- c("Digit 1", "Digit 2", "Digit 3", "Digit 4", "Digit 5")
Young_Pedal_Phalangeal_Indices <- Young_Pedal_Phalangeal_Indices[, c(5,1,2,3,4)]
  # Removing Unnecessary Columns
Young_Pedal_Phalangeal_Indices <- Young_Pedal_Phalangeal_Indices[,-c(2)]
  # Renaming a Column
colnames(Young_Pedal_Phalangeal_Indices)[2] ="Slope"
  # Extracting T-Value
Young_Pedal_Phalangeal_Indices$T_Value <- c(t_value_intrinsic_young_6, t_value_intrinsic_young_7, t_value_intrinsic_young_8, t_value_intrinsic_young_9, t_value_intrinsic_young_10)
  # Extracting P-Values
Young_Pedal_Phalangeal_Indices$P_Value <- c(p_value_intrinsic_young_6_mouse, p_value_intrinsic_young_7_mouse, p_value_intrinsic_young_8_mouse, p_value_intrinsic_young_9_mouse, p_value_intrinsic_young_10_mouse)

# Saving Tables as Mouse-Specific
Mouse_Manual_Phalangeal_Indices <- Young_Manual_Phalangeal_Indices
Mouse_Pedal_Phalangeal_Indices <- Young_Pedal_Phalangeal_Indices
```

## Extrinsic Measure - Mouse
```{r}
# Extrinsic Measure: Ontogenetic Allometry of Metapodial and Phalangeal Lengths [RECREATION OF FIGURE 4 in Druelle et. al (2018)]
  # The study used bivariate log-log regression of the individual digit ray bone lengths on total body mass. The slopes were compared to the isometric expectation of "0.333" for length v. mass. For this current version of the R script I will be examining the Metapodial and Phalangeal Lengths in respect to the Crown-rump Length, the scaling exponent for this will be "1.000".

# Formatting the Data - For Graphs (Mean & SD)
  # Forepaw Metapodials
model_extrinsic_druelle_1 = lm(HM1_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_2 = lm(HM2_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_3 = lm(HM3_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_4 = lm(HM4_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_5 = lm(HM5_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
  # Forepaw Phalanges
model_extrinsic_druelle_6 = lm(HP1_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_7 = lm(HP2_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_8 = lm(HP3_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_9 = lm(HP4_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_10 = lm(HP5_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
  # Hind Paw Metapodials
model_extrinsic_druelle_11 = lm(FM1_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_12 = lm(FM2_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_13 = lm(FM3_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_14 = lm(FM4_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_15 = lm(FM5_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
  # Hind Paw Phalanges
model_extrinsic_druelle_16 = lm(FP1_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_17 = lm(FP2_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_18 = lm(FP3_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_19 = lm(FP4_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_20 = lm(FP5_z_score ~ Crown_rump_length, data = mouse_autopod_data_z_score)

# Formatting the Data - Setting Null = 1.00 for 1D-1D Scaling (For T-Value & P-Value)
  # Forepaw Metapodials
model_extrinsic_druelle_1_offset = lm(HM1_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_2_offset = lm(HM2_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_3_offset = lm(HM3_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_4_offset = lm(HM4_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_5_offset = lm(HM5_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
  # Forepaw Phalanges
model_extrinsic_druelle_6_offset = lm(HP1_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)
model_extrinsic_druelle_7_offset = lm(HP2_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = mouse_autopod_data_z_score)

# Formatting the Data for Tidyverse
  # Changing Names
human_autopod_data_intrinsic_graph <- human_autopod_data_intrinsic %>% 
        rename("MANUAL DIGIT 1" = "HUMAN_MANUAL_DIGIT_1",
               "MANUAL DIGIT 2" = "HUMAN_MANUAL_DIGIT_2",
               "MANUAL DIGIT 3" = "HUMAN_MANUAL_DIGIT_3",
               "MANUAL DIGIT 4" = "HUMAN_MANUAL_DIGIT_4",
               "MANUAL DIGIT 5" = "HUMAN_MANUAL_DIGIT_5",
               "PEDAL DIGIT 1" = "HUMAN_PEDAL_DIGIT_1",
               "PEDAL DIGIT 2" = "HUMAN_PEDAL_DIGIT_2",
               "PEDAL DIGIT 3" = "HUMAN_PEDAL_DIGIT_3",
               "PEDAL DIGIT 4" = "HUMAN_PEDAL_DIGIT_4",
               "PEDAL DIGIT 5" = "HUMAN_PEDAL_DIGIT_5")
  # TidyVerse
human_measurements <- colnames(human_autopod_data_intrinsic_graph)[8:17]
human_autopod_data_tidy_intrinsic <- human_autopod_data_intrinsic_graph %>% pivot_longer(all_of(human_measurements), names_to = "measurement_id")

# Facet-Wrap() for Ontogenetic Changes in Phalangeal Indices
ggp_hpi <- ggplot(data = human_autopod_data_tidy_intrinsic, aes(timepoint, value)) +
  geom_point(color="magenta") + 
  labs(y = "Phalangeal Indices", x = "Embryonic Timepoint (Day)") +
  facet_wrap(~ measurement_id, nrow = 2)  + stat_smooth(method = 'lm', formula = y ~ x, geom = "smooth", color="magenta")
ggp_hpi

# Facet-Wrap() for Ontogenetic Changes in Phalangeal Indices + Adult Human Phalangeal Indices
  # Defining the Adult Human Phalangeal Indices
adult_human_pi <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=1420161400")
  # Combining with measurement_id
human_autopod_data_tidy_intrinsic_hline <- data.frame(measurement_id = unique(human_autopod_data_tidy_intrinsic$measurement_id), hline = adult_human_pi$phalangeal_indices_value)
  # Facet-Wrap()
ggp_hpi +
  geom_hline(data = human_autopod_data_tidy_intrinsic_hline, 
             aes(yintercept = hline))
```

## Summation Plots & Tables of Intrinsic Measurements - Human
```{r}
Young_Manual_Phalangeal_Indices <- data.frame(Autopod_Digit_Number = c("MANUAL DIGIT 1","MANUAL DIGIT 2", "MANUAL DIGIT 3", "MANUAL DIGIT 4", "MANUAL DIGIT 5"),
  Mean = c(mean_intrinsic_young_1, mean_intrinsic_young_2, mean_intrinsic_young_3, mean_intrinsic_young_4, mean_intrinsic_young_5),
  Upper_Bound = c(upper_bound_intrinsic_young_1, upper_bound_intrinsic_young_2, upper_bound_intrinsic_young_3, upper_bound_intrinsic_young_4, upper_bound_intrinsic_young_5),
  Lower_Bound = c(lower_bound_intrinsic_young_1, lower_bound_intrinsic_young_2, lower_bound_intrinsic_young_3, lower_bound_intrinsic_young_4, lower_bound_intrinsic_young_5)
  )

ggplot(Young_Manual_Phalangeal_Indices) +
  geom_bar(aes(x = Autopod_Digit_Number, y = Mean), stat = 'identity', fill = 'magenta1') +
  geom_errorbar(aes(x = Autopod_Digit_Number, ymin = Lower_Bound, ymax = Upper_Bound), width = 0.4) +
  geom_hline(yintercept = 0) +
  labs (y = "Slope (PI versus Embryonic Timepoint)", x = "") +
  scale_x_discrete(labels=c("DIGIT 1", "DIGIT 2", "DIGIT 3", "DIGIT 4", "DIGIT 5")) +
  ggtitle ("Human Manual Digits") + 
  theme(plot.title = element_text(face = "bold")) +
  theme(axis.text.x = element_text(face = "bold")) +
  theme(plot.title = element_text(hjust = 0.5))

Young_Pedal_Phalangeal_Indices <- data.frame(Autopod_Digit_Number = c("PEDAL DIGIT 1", "PEDAL DIGIT 2", "PEDAL DIGIT 3", "PEDAL DIGIT 4", "PEDAL DIGIT 5"),
  Mean = c(mean_intrinsic_young_6, mean_intrinsic_young_7, mean_intrinsic_young_8, mean_intrinsic_young_9, mean_intrinsic_young_10),
  Upper_Bound = c(upper_bound_intrinsic_young_6, upper_bound_intrinsic_young_7, upper_bound_intrinsic_young_8, upper_bound_intrinsic_young_9, upper_bound_intrinsic_young_10),
  Lower_Bound = c(lower_bound_intrinsic_young_6, lower_bound_intrinsic_young_7, lower_bound_intrinsic_young_8, lower_bound_intrinsic_young_9, lower_bound_intrinsic_young_10)
  )

ggplot(Young_Pedal_Phalangeal_Indices) +
  geom_bar(aes(x = Autopod_Digit_Number, y = Mean), stat = 'identity', fill = 'magenta3') +
  geom_errorbar(aes(x = Autopod_Digit_Number, ymin = Lower_Bound, ymax = Upper_Bound), width = 0.4) +
  geom_hline(yintercept = 0) +
  labs (y = "Slope (PI versus Embryonic Timepoint", x = "") +
    scale_x_discrete(labels=c("DIGIT 1", "DIGIT 2", "DIGIT 3", "DIGIT 4", "DIGIT 5")) +
  ggtitle ("Human Pedal Digits") + 
  theme(plot.title = element_text(face = "bold")) +
  theme(axis.text.x = element_text(face = "bold")) +
  theme(plot.title = element_text(hjust = 0.5))

# Manual PI Table
  # Adding Digit Names
Young_Manual_Phalangeal_Indices$Digit_Number <- c("Digit 1", "Digit 2", "Digit 3", "Digit 4", "Digit 5")
Young_Manual_Phalangeal_Indices <- Young_Manual_Phalangeal_Indices[, c(5,1,2,3,4)]
  # Removing Unnecessary Columns
Young_Manual_Phalangeal_Indices <- Young_Manual_Phalangeal_Indices[,-c(2)]
  # Renaming a Column
colnames(Young_Manual_Phalangeal_Indices)[2] ="Slope"
  # Extracting T-Value
Young_Manual_Phalangeal_Indices$T_Value <- c(t_value_intrinsic_young_1, t_value_intrinsic_young_2, t_value_intrinsic_young_3, t_value_intrinsic_young_4, t_value_intrinsic_young_5)
  # Extracting P-Values
Young_Manual_Phalangeal_Indices$P_Value <- c(p_value_intrinsic_young_1_human, p_value_intrinsic_young_2_human, p_value_intrinsic_young_3_human, p_value_intrinsic_young_4_human, p_value_intrinsic_young_5_human)

# Pedal PI Table
  # Adding Digit Names
Young_Pedal_Phalangeal_Indices$Digit_Number <- c("Digit 1", "Digit 2", "Digit 3", "Digit 4", "Digit 5")
Young_Pedal_Phalangeal_Indices <- Young_Pedal_Phalangeal_Indices[, c(5,1,2,3,4)]

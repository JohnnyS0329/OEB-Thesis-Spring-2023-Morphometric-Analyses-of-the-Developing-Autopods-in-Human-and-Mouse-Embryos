## Ontogenetic Allometry Graphs - Human
```{r}
# Formatting the Data for Tidyverse
# Renaming
human_autopod_data_extrinsic_graph <- human_autopod_data_z_score %>% 
        rename("HP1" = "HP1_z_score",
               "HP2" = "HP2_z_score",
               "HP3" = "HP3_z_score",
               "HP4" = "HP4_z_score",
               "HP5" = "HP5_z_score",
               "HM1" = "HM1_z_score",
               "HM2" = "HM2_z_score",
               "HM3" = "HM3_z_score",
               "HM4" = "HM4_z_score",
               "HM5" = "HM5_z_score",
               "FP1" = "FP1_z_score",
               "FP2" = "FP2_z_score",
               "FP3" = "FP3_z_score",
               "FP4" = "FP4_z_score",
               "FP5" = "FP5_z_score",
               "FM1" = "FM1_z_score",
               "FM2" = "FM2_z_score",
               "FM3" = "FM3_z_score",
               "FM4" = "FM4_z_score",
               "FM5" = "FM5_z_score")
  # Tidyverse
human_measurements <- colnames(human_autopod_data_extrinsic_graph)[8:27]
human_autopod_data_tidy_z_score <- human_autopod_data_extrinsic_graph %>% pivot_longer(all_of(human_measurements), names_to = "measurement_id") %>% select(-notes) %>% select(-timepoint)

# Facet-Wrap() for Ontogenetic Allometry
ggplot(data = human_autopod_data_tidy_z_score, aes(Crown_rump_length, value)) +
  geom_point(color = "magenta") + 
  labs(title = "Human Allometry",
       y = "Z-score Transformed Length", x = "Z-score Transformed Crown-rump Length") + 
  facet_wrap(~ measurement_id, nrow = 4)  + stat_smooth(method = 'lm', formula = y ~ x, geom = "smooth", color = "magenta")
```


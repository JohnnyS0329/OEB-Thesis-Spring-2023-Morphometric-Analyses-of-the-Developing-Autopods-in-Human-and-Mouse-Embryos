## Extrinsic Measure - Human
```{r}
# Extrinsic Measure: Ontogenetic Allometry of Metapodial and Phalangeal Lengths [RECREATION OF FIGURE 4 in Druelle et. al (2018)]
  # The study used bivariate log-log regression of the individual digit ray bone lengths on total body mass. The slopes were compared to the isometric expectation of "0.333" for length v. mass. For this current version of the R script I will be examining the Metapodial and Phalangeal Lengths in respect to the Crown-rump Length, the scaling exponent for this will be "1.000".

# Formatting the Data - For Graphs (Mean & SD)
  # Hand Metapodials
model_extrinsic_druelle_1 = lm(HM1_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_2 = lm(HM2_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_3 = lm(HM3_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_4 = lm(HM4_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_5 = lm(HM5_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
  # Hand Phalanges
model_extrinsic_druelle_6 = lm(HP1_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_7 = lm(HP2_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_8 = lm(HP3_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_9 = lm(HP4_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_10 = lm(HP5_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
  # Foot Metapodials
model_extrinsic_druelle_11 = lm(FM1_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_12 = lm(FM2_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_13 = lm(FM3_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_14 = lm(FM4_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_15 = lm(FM5_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
  # Foot Phalanges
model_extrinsic_druelle_16 = lm(FP1_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_17 = lm(FP2_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_18 = lm(FP3_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_19 = lm(FP4_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_20 = lm(FP5_z_score ~ Crown_rump_length, data = human_autopod_data_z_score)

# Formatting the Data - Setting Null = 1.00 for 1D-1D Scaling (For T-Value & P-Value)
  # Hand Metapodials
model_extrinsic_druelle_1_offset = lm(HM1_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_2_offset = lm(HM2_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_3_offset = lm(HM3_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_4_offset = lm(HM4_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_5_offset = lm(HM5_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
  # Hand Phalanges
model_extrinsic_druelle_6_offset = lm(HP1_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_7_offset = lm(HP2_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_8_offset = lm(HP3_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_9_offset = lm(HP4_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_10_offset = lm(HP5_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
  # Foot Metapodials
model_extrinsic_druelle_11_offset = lm(FM1_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_12_offset = lm(FM2_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_13_offset = lm(FM3_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_14_offset = lm(FM4_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_15_offset = lm(FM5_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
  # Foot Phalanges
model_extrinsic_druelle_16_offset = lm(FP1_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_17_offset = lm(FP2_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_18_offset = lm(FP3_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_19_offset = lm(FP4_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)
model_extrinsic_druelle_20_offset = lm(FP5_z_score ~ Crown_rump_length, offset = 1.00*Crown_rump_length, data = human_autopod_data_z_score)

# Extracting the Numbers
m = summary(model_extrinsic_druelle_1)
  mean_extrinsic_druelle_1 <- m$coefficients[2,1]
  upper_bound_extrinsic_druelle_1 <- confint(model_extrinsic_druelle_1, "Crown_rump_length")[2]
  lower_bound_extrinsic_druelle_1 <- confint(model_extrinsic_druelle_1, "Crown_rump_length")[1]

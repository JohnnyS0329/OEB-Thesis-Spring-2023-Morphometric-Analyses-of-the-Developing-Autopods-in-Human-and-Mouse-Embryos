## Percentage Measurement Precision
```{r, include=FALSE}
# Retrieve Recent Data
original_data <- gsheet2tbl(url="docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=661840542")
remeasured_data <- gsheet2tbl(url="https://docs.google.com/spreadsheets/d/1mzXWDMwi8ok37ChaK3bLmeHBsXdeCAxKSKo2__jbBxw/edit#gid=750827456")
## Percent Measurement Precision
percent_precision <- function(x,y) {
  output = (100-((abs(x-y))/(x))*100)
  return(output)
}

# Running Percent Precision Function + Saving it as a Data Frame
autopod_percent_precision <- percent_precision(original_data[c(8:47)], remeasured_data[c(8:47)]) %>% 
  cbind(original_data[1:2], .) %>% view()
```

## Z-Scores - Human
```{r}
# Z-Scores
  # Excluding data w/o CRL
human_autopod_data_CRL <- human_autopod_data_avg[-c(2,11),]
  # Making the Z-Scores
human_autopod_data_z_score <- human_autopod_data_CRL[1:6]

human_autopod_data_z_score$Crown_rump_length <- (human_autopod_data_CRL$Crown_rump_length - mean(human_autopod_data_CRL$Crown_rump_length, na.rm = TRUE)) / sd(human_autopod_data_CRL$Crown_rump_length, na.rm = TRUE)

human_autopod_data_z_score$HP1_z_score <- (human_autopod_data_CRL$HP1_avg - mean(human_autopod_data_CRL$HP1_avg)) / sd(human_autopod_data_CRL$HP1_avg)
human_autopod_data_z_score$HP2_z_score <- (human_autopod_data_CRL$HP2_avg - mean(human_autopod_data_CRL$HP2_avg)) / sd(human_autopod_data_CRL$HP2_avg)
human_autopod_data_z_score$HP3_z_score <- (human_autopod_data_CRL$HP3_avg - mean(human_autopod_data_CRL$HP3_avg)) / sd(human_autopod_data_CRL$HP3_avg)
human_autopod_data_z_score$HP4_z_score <- (human_autopod_data_CRL$HP4_avg - mean(human_autopod_data_CRL$HP4_avg)) / sd(human_autopod_data_CRL$HP4_avg)
human_autopod_data_z_score$HP5_z_score <- (human_autopod_data_CRL$HP5_avg - mean(human_autopod_data_CRL$HP5_avg)) / sd(human_autopod_data_CRL$HP5_avg)

human_autopod_data_z_score$HM1_z_score <- (human_autopod_data_CRL$HM1_avg - mean(human_autopod_data_CRL$HM1_avg)) / sd(human_autopod_data_CRL$HM1_avg)
human_autopod_data_z_score$HM2_z_score <- (human_autopod_data_CRL$HM2_avg - mean(human_autopod_data_CRL$HM2_avg)) / sd(human_autopod_data_CRL$HM2_avg)
human_autopod_data_z_score$HM3_z_score <- (human_autopod_data_CRL$HM3_avg - mean(human_autopod_data_CRL$HM3_avg)) / sd(human_autopod_data_CRL$HM3_avg)
human_autopod_data_z_score$HM4_z_score <- (human_autopod_data_CRL$HM4_avg - mean(human_autopod_data_CRL$HM4_avg)) / sd(human_autopod_data_CRL$HM4_avg)
human_autopod_data_z_score$HM5_z_score <- (human_autopod_data_CRL$HM5_avg - mean(human_autopod_data_CRL$HM5_avg)) / sd(human_autopod_data_CRL$HM5_avg)

human_autopod_data_z_score$FP1_z_score <- (human_autopod_data_CRL$FP1_avg - mean(human_autopod_data_CRL$FP1_avg)) / sd(human_autopod_data_CRL$FP1_avg)
human_autopod_data_z_score$FP2_z_score <- (human_autopod_data_CRL$FP2_avg - mean(human_autopod_data_CRL$FP2_avg)) / sd(human_autopod_data_CRL$FP2_avg)
human_autopod_data_z_score$FP3_z_score <- (human_autopod_data_CRL$FP3_avg - mean(human_autopod_data_CRL$FP3_avg)) / sd(human_autopod_data_CRL$FP3_avg)
human_autopod_data_z_score$FP4_z_score <- (human_autopod_data_CRL$FP4_avg - mean(human_autopod_data_CRL$FP4_avg)) / sd(human_autopod_data_CRL$FP4_avg)
human_autopod_data_z_score$FP5_z_score <- (human_autopod_data_CRL$FP5_avg - mean(human_autopod_data_CRL$FP5_avg)) / sd(human_autopod_data_CRL$FP5_avg)

human_autopod_data_z_score$FM1_z_score <- (human_autopod_data_CRL$FM1_avg - mean(human_autopod_data_CRL$FM1_avg)) / sd(human_autopod_data_CRL$FM1_avg)
human_autopod_data_z_score$FM2_z_score <- (human_autopod_data_CRL$FM2_avg - mean(human_autopod_data_CRL$FM2_avg)) / sd(human_autopod_data_CRL$FM2_avg)
human_autopod_data_z_score$FM3_z_score <- (human_autopod_data_CRL$FM3_avg - mean(human_autopod_data_CRL$FM3_avg)) / sd(human_autopod_data_CRL$FM3_avg)
human_autopod_data_z_score$FM4_z_score <- (human_autopod_data_CRL$FM4_avg - mean(human_autopod_data_CRL$FM4_avg)) / sd(human_autopod_data_CRL$FM4_avg)
human_autopod_data_z_score$FM5_z_score <- (human_autopod_data_CRL$FM5_avg - mean(human_autopod_data_CRL$FM5_avg)) / sd(human_autopod_data_CRL$FM5_avg)
```

## Growth Curves - Human
```{r}
# Facet_Wrap for Human Growth Curves by Elements - Linear
ggplot(data = human_autopod_data_tidy_merged_avg, aes(timepoint, value)) +
  geom_point(color="magenta") + 
  labs(y = "Length (mm)", x = "Embyronic Timepoint (Day)") + 
  facet_wrap(~ element, nrow = 4)  + stat_smooth(method = 'lm', formula = y ~ x, geom = "smooth", color="magenta")

# Facet_Wrap for Human Growth Curves by Elements & Sex - Linear
ggplot(data = human_autopod_data_tidy_merged_avg, aes(timepoint, value, color = sex)) +
  geom_point() + 
  labs(y = "Length (mm)", x = "Embyronic Timepoint (Day)",
       color = "Sex") + 
  facet_wrap(~ element, nrow = 4) + stat_smooth(method = 'lm', formula = y ~ x, geom = "smooth")
```

## Intrinsic Measure - Human
```{r}
# Intrinsic Measure: Ontogenetic Scaling of Manual and Pedal Phalangeal Indices [RECREATION OF FIGURES 5 & 6]
  # In terms of the ontogenetic scaling for this paper, Young focuses on comparing the Phalangeal Indices in comparison to Relative Age. For this R script I just decided to stick with comparing the Phalangeal Indices in comparison to the Actual Age also known as the Embryonic Day in this comparison.

  # The formula for the this version of the "Phalangeal Index" is:
    # Phalangeal Index = (Length of a Ray's Phalanges)/(Length of a Ray's Metapodial)

  # Formatting the Data
human_autopod_data_intrinsic <- human_autopod_data_avg[1:7]

human_autopod_data_intrinsic$HUMAN_MANUAL_DIGIT_1 <- ((human_autopod_data_avg$HP1_avg/human_autopod_data_avg$HM1_avg))
human_autopod_data_intrinsic$HUMAN_MANUAL_DIGIT_2 <- ((human_autopod_data_avg$HP2_avg/human_autopod_data_avg$HM2_avg))
human_autopod_data_intrinsic$HUMAN_MANUAL_DIGIT_3 <- ((human_autopod_data_avg$HP3_avg/human_autopod_data_avg$HM3_avg))
human_autopod_data_intrinsic$HUMAN_MANUAL_DIGIT_4 <- ((human_autopod_data_avg$HP4_avg/human_autopod_data_avg$HM4_avg))
human_autopod_data_intrinsic$HUMAN_MANUAL_DIGIT_5 <- ((human_autopod_data_avg$HP5_avg/human_autopod_data_avg$HM5_avg))

human_autopod_data_intrinsic$HUMAN_PEDAL_DIGIT_1 <- ((human_autopod_data_avg$FP1_avg/human_autopod_data_avg$FM1_avg))
human_autopod_data_intrinsic$HUMAN_PEDAL_DIGIT_2 <- ((human_autopod_data_avg$FP2_avg/human_autopod_data_avg$FM2_avg))
human_autopod_data_intrinsic$HUMAN_PEDAL_DIGIT_3 <- ((human_autopod_data_avg$FP3_avg/human_autopod_data_avg$FM3_avg))
human_autopod_data_intrinsic$HUMAN_PEDAL_DIGIT_4 <- ((human_autopod_data_avg$FP4_avg/human_autopod_data_avg$FM4_avg))
human_autopod_data_intrinsic$HUMAN_PEDAL_DIGIT_5 <- ((human_autopod_data_avg$FP5_avg/human_autopod_data_avg$FM5_avg))

human_autopod_data_intrinsic$timepoint <- as.numeric(as.character(human_autopod_data_intrinsic$timepoint))

# Making the Models
    # Manual Digit 1 PI
model_intrinsic_young_1 = lm(HUMAN_MANUAL_DIGIT_1 ~ timepoint, data = human_autopod_data_intrinsic)
    # Manual Digit 2 PI
model_intrinsic_young_2 = lm(HUMAN_MANUAL_DIGIT_2 ~ timepoint, data = human_autopod_data_intrinsic)
    # Manual Digit 3 PI
model_intrinsic_young_3 = lm(HUMAN_MANUAL_DIGIT_3 ~ timepoint, data = human_autopod_data_intrinsic)
    # Manual Digit 4 PI
model_intrinsic_young_4 = lm(HUMAN_MANUAL_DIGIT_4 ~ timepoint, data = human_autopod_data_intrinsic)
    # Manual Digit 5 PI
model_intrinsic_young_5 = lm(HUMAN_MANUAL_DIGIT_5 ~ timepoint, data = human_autopod_data_intrinsic)
    # Pedal Digit 1 PI
model_intrinsic_young_6 = lm(HUMAN_PEDAL_DIGIT_1 ~ timepoint, data = human_autopod_data_intrinsic)
    # Pedal Digit 2 PI
model_intrinsic_young_7 = lm(HUMAN_PEDAL_DIGIT_2 ~ timepoint, data = human_autopod_data_intrinsic)
    # Pedal Digit 3 PI
model_intrinsic_young_8 = lm(HUMAN_PEDAL_DIGIT_3 ~ timepoint, data = human_autopod_data_intrinsic)
    # Pedal Digit 4 PI
model_intrinsic_young_9 = lm(HUMAN_PEDAL_DIGIT_4 ~ timepoint, data = human_autopod_data_intrinsic)
    # Pedal Digit 5 PI
model_intrinsic_young_10 = lm(HUMAN_PEDAL_DIGIT_5 ~ timepoint, data = human_autopod_data_intrinsic)

# Extracting Numbers
m = summary(model_intrinsic_young_1)
  mean_intrinsic_young_1 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_1 <- confint(model_intrinsic_young_1, "timepoint")[2]
  lower_bound_intrinsic_young_1 <- confint(model_intrinsic_young_1, "timepoint")[1]
  t_value_intrinsic_young_1 <- m$coefficients[2,3]
  p_value_intrinsic_young_1_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_2)
  mean_intrinsic_young_2 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_2 <- confint(model_intrinsic_young_2, "timepoint")[2]
  lower_bound_intrinsic_young_2 <- confint(model_intrinsic_young_2, "timepoint")[1]
  t_value_intrinsic_young_2 <- m$coefficients[2,3]
  p_value_intrinsic_young_2_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_3)
  mean_intrinsic_young_3 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_3 <- confint(model_intrinsic_young_3, "timepoint")[2]
  lower_bound_intrinsic_young_3 <- confint(model_intrinsic_young_3, "timepoint")[1]
  t_value_intrinsic_young_3 <- m$coefficients[2,3]
  p_value_intrinsic_young_3_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_4)
  mean_intrinsic_young_4 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_4 <- confint(model_intrinsic_young_4, "timepoint")[2]
  lower_bound_intrinsic_young_4 <- confint(model_intrinsic_young_4, "timepoint")[1]
  t_value_intrinsic_young_4 <- m$coefficients[2,3]
  p_value_intrinsic_young_4_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_5)
  mean_intrinsic_young_5 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_5 <- confint(model_intrinsic_young_5, "timepoint")[2]
  lower_bound_intrinsic_young_5 <- confint(model_intrinsic_young_5, "timepoint")[1]
  t_value_intrinsic_young_5 <- m$coefficients[2,3]
  p_value_intrinsic_young_5_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_6)
  mean_intrinsic_young_6 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_6 <- confint(model_intrinsic_young_6, "timepoint")[2]
  lower_bound_intrinsic_young_6 <- confint(model_intrinsic_young_6, "timepoint")[1]
  t_value_intrinsic_young_6 <- m$coefficients[2,3]
  p_value_intrinsic_young_6_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_7)
  mean_intrinsic_young_7 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_7 <- confint(model_intrinsic_young_7, "timepoint")[2]
  lower_bound_intrinsic_young_7 <- confint(model_intrinsic_young_7, "timepoint")[1]
  t_value_intrinsic_young_7 <- m$coefficients[2,3]
  p_value_intrinsic_young_7_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_8)
  mean_intrinsic_young_8 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_8 <- confint(model_intrinsic_young_8, "timepoint")[2]
  lower_bound_intrinsic_young_8 <- confint(model_intrinsic_young_8, "timepoint")[1]
  t_value_intrinsic_young_8 <- m$coefficients[2,3]
  p_value_intrinsic_young_8_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_9)
  mean_intrinsic_young_9 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_9 <- confint(model_intrinsic_young_9, "timepoint")[2]
  lower_bound_intrinsic_young_9 <- confint(model_intrinsic_young_9, "timepoint")[1]
  t_value_intrinsic_young_9 <- m$coefficients[2,3]
  p_value_intrinsic_young_9_human <- m$coefficients[2,4]
  
m = summary(model_intrinsic_young_10)
  mean_intrinsic_young_10 <- m$coefficients[2,1]
  upper_bound_intrinsic_young_10 <- confint(model_intrinsic_young_10, "timepoint")[2]
  lower_bound_intrinsic_young_10 <- confint(model_intrinsic_young_10, "timepoint")[1]
  t_value_intrinsic_young_10 <- m$coefficients[2,3]
  p_value_intrinsic_young_10_human <- m$coefficients[2,4]
```


    # Extracting R-Squared Value
Druelle_Pedal_Comparison_to_CRL$T_Value <- c(t_value_extrinsic_druelle_11, t_value_extrinsic_druelle_16, t_value_extrinsic_druelle_12,
t_value_extrinsic_druelle_17, t_value_extrinsic_druelle_13,
t_value_extrinsic_druelle_18, t_value_extrinsic_druelle_14,
t_value_extrinsic_druelle_19, t_value_extrinsic_druelle_15,
t_value_extrinsic_druelle_20)
    # Extracting P-Value
Druelle_Pedal_Comparison_to_CRL$P_Value <- c(p_value_extrinsic_druelle_11_human, p_value_extrinsic_druelle_16_human, p_value_extrinsic_druelle_12_human, 
p_value_extrinsic_druelle_17_human, p_value_extrinsic_druelle_13_human,
p_value_extrinsic_druelle_18_human, p_value_extrinsic_druelle_14_human,
p_value_extrinsic_druelle_19_human, p_value_extrinsic_druelle_15_human,
p_value_extrinsic_druelle_20_human)

# Saving Tables as Human-Specific
Human_Manual_Comparison_to_CRL <- Druelle_Manual_Comparison_to_CRL
Human_Pedal_Comparison_to_CRL <- Druelle_Pedal_Comparison_to_CRL

## Z-Scores - Mouse
```{r}
## Z-Scores
mouse_autopod_data_z_score <- mouse_autopod_data_avg[1:6]

mouse_autopod_data_z_score$Crown_rump_length <- (mouse_autopod_data_avg$Crown_rump_length - mean(mouse_autopod_data_avg$Crown_rump_length)) / sd(mouse_autopod_data_avg$Crown_rump_length)

mouse_autopod_data_z_score$HP1_z_score <- (mouse_autopod_data_avg$HP1_avg - mean(mouse_autopod_data_avg$HP1_avg)) / sd(mouse_autopod_data_avg$HP1_avg)
mouse_autopod_data_z_score$HP2_z_score <- (mouse_autopod_data_avg$HP2_avg - mean(mouse_autopod_data_avg$HP2_avg)) / sd(mouse_autopod_data_avg$HP2_avg)
mouse_autopod_data_z_score$HP3_z_score <- (mouse_autopod_data_avg$HP3_avg - mean(mouse_autopod_data_avg$HP3_avg)) / sd(mouse_autopod_data_avg$HP3_avg)
mouse_autopod_data_z_score$HP4_z_score <- (mouse_autopod_data_avg$HP4_avg - mean(mouse_autopod_data_avg$HP4_avg)) / sd(mouse_autopod_data_avg$HP4_avg)
mouse_autopod_data_z_score$HP5_z_score <- (mouse_autopod_data_avg$HP5_avg - mean(mouse_autopod_data_avg$HP5_avg)) / sd(mouse_autopod_data_avg$HP5_avg)

mouse_autopod_data_z_score$HM1_z_score <- (mouse_autopod_data_avg$HM1_avg - mean(mouse_autopod_data_avg$HM1_avg)) / sd(mouse_autopod_data_avg$HM1_avg)
mouse_autopod_data_z_score$HM2_z_score <- (mouse_autopod_data_avg$HM2_avg - mean(mouse_autopod_data_avg$HM2_avg)) / sd(mouse_autopod_data_avg$HM2_avg)
mouse_autopod_data_z_score$HM3_z_score <- (mouse_autopod_data_avg$HM3_avg - mean(mouse_autopod_data_avg$HM3_avg)) / sd(mouse_autopod_data_avg$HM3_avg)
mouse_autopod_data_z_score$HM4_z_score <- (mouse_autopod_data_avg$HM4_avg - mean(mouse_autopod_data_avg$HM4_avg)) / sd(mouse_autopod_data_avg$HM4_avg)
mouse_autopod_data_z_score$HM5_z_score <- (mouse_autopod_data_avg$HM5_avg - mean(mouse_autopod_data_avg$HM5_avg)) / sd(mouse_autopod_data_avg$HM5_avg)

mouse_autopod_data_z_score$FP1_z_score <- (mouse_autopod_data_avg$FP1_avg - mean(mouse_autopod_data_avg$FP1_avg)) / sd(mouse_autopod_data_avg$FP1_avg)
mouse_autopod_data_z_score$FP2_z_score <- (mouse_autopod_data_avg$FP2_avg - mean(mouse_autopod_data_avg$FP2_avg)) / sd(mouse_autopod_data_avg$FP2_avg)
mouse_autopod_data_z_score$FP3_z_score <- (mouse_autopod_data_avg$FP3_avg - mean(mouse_autopod_data_avg$FP3_avg)) / sd(mouse_autopod_data_avg$FP3_avg)
mouse_autopod_data_z_score$FP4_z_score <- (mouse_autopod_data_avg$FP4_avg - mean(mouse_autopod_data_avg$FP4_avg)) / sd(mouse_autopod_data_avg$FP4_avg)
mouse_autopod_data_z_score$FP5_z_score <- (mouse_autopod_data_avg$FP5_avg - mean(mouse_autopod_data_avg$FP5_avg)) / sd(mouse_autopod_data_avg$FP5_avg)

mouse_autopod_data_z_score$FM1_z_score <- (mouse_autopod_data_avg$FM1_avg - mean(mouse_autopod_data_avg$FM1_avg)) / sd(mouse_autopod_data_avg$FM1_avg)
mouse_autopod_data_z_score$FM2_z_score <- (mouse_autopod_data_avg$FM2_avg - mean(mouse_autopod_data_avg$FM2_avg)) / sd(mouse_autopod_data_avg$FM2_avg)
mouse_autopod_data_z_score$FM3_z_score <- (mouse_autopod_data_avg$FM3_avg - mean(mouse_autopod_data_avg$FM3_avg)) / sd(mouse_autopod_data_avg$FM3_avg)
mouse_autopod_data_z_score$FM4_z_score <- (mouse_autopod_data_avg$FM4_avg - mean(mouse_autopod_data_avg$FM4_avg)) / sd(mouse_autopod_data_avg$FM4_avg)
mouse_autopod_data_z_score$FM5_z_score <- (mouse_autopod_data_avg$FM5_avg - mean(mouse_autopod_data_avg$FM5_avg)) / sd(mouse_autopod_data_avg$FM5_avg)
```

